        <StackLayout>
            <Label x:Name="LabelIpServer" Text="{Binding IpServer}"/>
            <Label x:Name="LabelPortConnection" Text="{Binding PortInMasterValue}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>